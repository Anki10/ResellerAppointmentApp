<?xml version="1.0" encoding="UTF-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="100dp" />
            <stroke android:width="1dip" android:color="@color/dialog_reject_color" />
            <gradient android:angle="-90" android:startColor="@color/dialog_reject_color" android:endColor="@color/dialog_reject_color"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="100dp" />
            <stroke android:width="1dip" android:color="@color/dialog_reject_color" />
            <solid android:color="@color/dialog_reject_color"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="100dp" />
            <stroke android:width="1dip" android:color="@color/dialog_reject_color" />
            <gradient android:angle="-90" android:startColor="@color/dialog_reject_color" android:endColor="@color/dialog_reject_color" />
        </shape>
    </item>
</selector>
